// â”€â”€ TERMINAL TYPEWRITER ANIMATION â”€â”€
const terminalLines = [
  { type: 'prompt', text: 'claude "×¦×•×¨ ×œ×™ ××¤×œ×™×§×¦×™×™×ª × ×™×”×•×œ ×œ×§×•×—×•×ª"' },
  { type: 'info',   text: 'âœ¦ Mode: Agent  |  Context: CLAUDE.md' },
  { type: 'info',   text: 'âœ¦ Model: claude-sonnet-4-6' },
  { type: 'blank' },
  { type: 'success',text: 'âœ“ Reading CLAUDE.md...' },
  { type: 'success',text: 'âœ“ Planning architecture...' },
  { type: 'dim',    text: '  â†’ Creating app/layout.tsx' },
  { type: 'dim',    text: '  â†’ Creating app/dashboard/page.tsx' },
  { type: 'dim',    text: '  â†’ Creating components/CustomerTable.tsx' },
  { type: 'success',text: 'âœ“ Setting up Supabase...' },
  { type: 'dim',    text: '  â†’ Creating tables: customers, leads' },
  { type: 'dim',    text: '  â†’ Enabling Row Level Security' },
  { type: 'success',text: 'âœ“ Building authentication...' },
  { type: 'success',text: 'âœ“ Writing API endpoints...' },
  { type: 'success',text: 'âœ“ Running tests... 8/8 âœ“' },
  { type: 'blank' },
  { type: 'highlight', text: 'âœ“ Deploying to Vercel...' },
  { type: 'url',    text: '  ğŸš€ Live: https://my-crm-app.vercel.app' },
  { type: 'dim',    text: '  âœ“ Build completed in 2m 34s' },
  { type: 'cursor' },
];

function renderTerminal() {
  const body = document.getElementById('terminalBody');
  if (!body) return;
  body.innerHTML = '';
  let delay = 0;

  terminalLines.forEach((line, i) => {
    const el = document.createElement('div');
    el.className = 't-line';
    el.style.opacity = '0';
    el.style.transition = 'opacity 0.2s';

    if (line.type === 'blank') {
      el.innerHTML = '&nbsp;';
    } else if (line.type === 'cursor') {
      el.innerHTML = '<span class="t-prompt">â€º</span> <span class="t-cursor"></span>';
    } else if (line.type === 'prompt') {
      el.innerHTML = `<span class="t-prompt">â€º</span> <span class="t-cmd">claude <span style="color:var(--yellow)">"${line.text.replace('claude ', '')}"</span></span>`;
    } else if (line.type === 'success') {
      el.innerHTML = `<span class="t-success">${line.text}</span>`;
    } else if (line.type === 'info') {
      el.innerHTML = `<span class="t-info">${line.text}</span>`;
    } else if (line.type === 'dim') {
      el.innerHTML = `<span class="t-dim">${line.text}</span>`;
    } else if (line.type === 'highlight') {
      el.innerHTML = `<span class="t-highlight">${line.text}</span>`;
    } else if (line.type === 'url') {
      el.innerHTML = `<span class="t-url">${line.text}</span>`;
    }

    body.appendChild(el);

    setTimeout(() => { el.style.opacity = '1'; }, delay);
    delay += line.type === 'blank' ? 80 : 180;
  });
}

