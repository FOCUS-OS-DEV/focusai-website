  <script src="https://cdn.jsdelivr.net/npm/gsap@3.12.5/dist/ScrollTrigger.min.js" defer></script>
  <style>
    *{margin:0;padding:0;box-sizing:border-box}
    :root {
      --bg: #0a0a0f;
      --bg2: #0f1117;
      --card: #11151f;
      --border: rgba(255,255,255,0.07);
      --border2: rgba(255,255,255,0.04);
      --text: #e8eaf0;
      --text2: #8890a4;
      --text3: #454e64;
      --green: #3fb950;
      --blue: #58a6ff;
      --purple: #a855f7;
      --purple-glow: rgba(168,85,247,0.25);
      --gold: #f0b429;
      --mono: 'JetBrains Mono', monospace;
    }
    html { direction: rtl; scroll-behavior: smooth; }
    body { font-family: 'Heebo', sans-serif; background: var(--bg); color: var(--text); overflow-x: hidden; line-height: 1.6; }
    .mono { font-family: var(--mono); }

    /* â”€â”€ ANIMATED BG ORBS â”€â”€ */
    .bg-canvas { position: fixed; inset: 0; z-index: 0; pointer-events: none; overflow: hidden; }
    .orb { position: absolute; border-radius: 50%; will-change: transform; }
    .orb-1 { width: 800px; height: 800px; background: radial-gradient(circle, #a855f7, transparent 70%); filter: blur(140px); opacity: 0.07; top: -300px; right: -250px; animation: orbFloat 24s ease-in-out infinite; }
    .orb-2 { width: 600px; height: 600px; background: radial-gradient(circle, #58a6ff, transparent 70%); filter: blur(130px); opacity: 0.05; bottom: 10%; left: -200px; animation: orbFloat 30s ease-in-out infinite reverse; }
    .orb-3 { width: 500px; height: 500px; background: radial-gradient(circle, #3fb950, transparent 70%); filter: blur(110px); opacity: 0.04; top: 55%; right: 25%; animation: orbFloat 20s ease-in-out infinite 6s; }
    @keyframes orbFloat { 0%,100%{transform:translate(0,0) scale(1)} 33%{transform:translate(-50px,70px) scale(1.1)} 66%{transform:translate(40px,-50px) scale(0.95)} }

    /* â”€â”€ NAV â”€â”€ */
    .nav { position: sticky; top: 0; z-index: 200; background: rgba(10,10,15,0.88); backdrop-filter: blur(24px); border-bottom: 1px solid var(--border); }
    .nav-inner { max-width: 1180px; margin: 0 auto; padding: 0 24px; display: flex; align-items: center; gap: 24px; height: 56px; }
    .nav-logo img { height: 28px; display: block; }
    .nav-links { display: flex; gap: 4px; flex: 1; }
    .nav-link { font-size: 13px; color: var(--text2); padding: 5px 12px; border-radius: 6px; text-decoration: none; transition: all .2s; white-space: nowrap; }
    .nav-link:hover { color: var(--text); background: var(--border2); }
    .nav-cta { background: var(--purple); color: #fff; padding: 8px 20px; border-radius: 8px; font-size: 14px; font-weight: 700; text-decoration: none; transition: opacity .2s; white-space: nowrap; }
    .nav-cta:hover { opacity: .85; }
    @media(max-width:768px){ .nav-links { display: none; } .nav-cta { padding: 7px 14px; font-size: 13px; } }

    /* â”€â”€ HERO â”€â”€ */
    .hero-wrapper { position: relative; z-index: 1; padding: 72px 24px 56px; max-width: 1180px; margin: 0 auto; }
    .hero-eyebrow { display: inline-flex; align-items: center; gap: 8px; background: rgba(168,85,247,0.1); border: 1px solid rgba(168,85,247,0.3); border-radius: 100px; padding: 5px 14px 5px 10px; font-family: var(--mono); font-size: 11px; color: var(--purple); letter-spacing: .05em; margin-bottom: 28px; }
    .hero-eyebrow-dot { width: 6px; height: 6px; border-radius: 50%; background: var(--purple); animation: pulse 2s ease-in-out infinite; }
    @keyframes pulse { 0%,100%{opacity:1;transform:scale(1)} 50%{opacity:.5;transform:scale(.8)} }
    .hero-h1 { font-size: clamp(2.8rem,5.5vw,5rem); font-weight: 900; line-height: 1.08; letter-spacing: -.02em; margin-bottom: 20px; }
    .hero-h1 em { font-style: normal; background: linear-gradient(135deg, #a855f7, #58a6ff); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text; }
    .hero-sub { font-size: 1.15rem; color: var(--text2); max-width: 560px; margin-bottom: 36px; line-height: 1.7; }
    .hero-actions { display: flex; gap: 12px; flex-wrap: wrap; margin-bottom: 56px; }
    .btn-primary { background: var(--purple); color: #fff; padding: 14px 32px; border-radius: 10px; font-size: 16px; font-weight: 700; text-decoration: none; transition: all .25s; display: inline-flex; align-items: center; gap: 8px; }
    .btn-primary:hover { background: #9333ea; transform: translateY(-1px); box-shadow: 0 12px 32px rgba(168,85,247,0.35); }
    .btn-ghost { color: var(--text2); padding: 14px 28px; border-radius: 10px; font-size: 15px; font-weight: 500; text-decoration: none; border: 1px solid var(--border); transition: all .25s; display: inline-flex; align-items: center; gap: 8px; }
    .btn-ghost:hover { color: var(--text); border-color: rgba(255,255,255,0.15); background: var(--border2); }

    /* â”€â”€ BENTO GRID â”€â”€ */
    .hero-bento { display: grid; grid-template-columns: 2fr 1fr 1fr; grid-template-rows: 220px 160px; gap: 12px; }
    @media(max-width:1024px){ .hero-bento { grid-template-columns: 1fr 1fr; grid-template-rows: auto auto auto; } .bento-terminal { grid-column: 1/3; grid-row: 1; min-height: 220px; } .bento-stats { grid-column: 1; grid-row: 2; } .bento-trust { grid-column: 2; grid-row: 2; } .bento-guarantee { grid-column: 1; grid-row: 3; } .bento-grads { grid-column: 2; grid-row: 3; } }
    @media(max-width:640px){ .hero-bento { grid-template-columns: 1fr; grid-template-rows: auto; } .hero-bento > * { grid-column: 1 !important; grid-row: auto !important; min-height: auto !important; } }
    .bento-card { background: rgba(255,255,255,0.025); border: 1px solid var(--border); border-radius: 16px; padding: 20px; backdrop-filter: blur(20px); overflow: hidden; position: relative; transition: border-color .3s; }
    .bento-card:hover { border-color: rgba(255,255,255,0.12); }

    /* Terminal bento */
    .bento-terminal { grid-column: 1; grid-row: 1/3; padding: 0; display: flex; flex-direction: column; }
    .term-bar { display: flex; align-items: center; gap: 8px; padding: 10px 16px; background: rgba(0,0,0,0.3); border-bottom: 1px solid var(--border); border-radius: 16px 16px 0 0; flex-shrink: 0; }
    .term-title { font-family: var(--mono); font-size: 11px; color: var(--text2); flex: 1; }
    .term-body { flex: 1; padding: 18px 20px; font-family: var(--mono); font-size: 12.5px; line-height: 1.8; overflow: hidden; }
    .term-line { display: flex; gap: 10px; align-items: flex-start; }
    .term-prompt { color: var(--green); flex-shrink: 0; }
    .term-cmd { color: var(--text); }
    .term-cmd .kw { color: var(--purple); }
    .term-cmd .str { color: var(--gold); }
    .term-cmd .fn { color: var(--blue); }
    .term-cmd .cmt { color: var(--text3); }
    .term-output { color: var(--text2); padding-right: 20px; }
    .term-cursor { display: inline-block; width: 8px; height: 15px; background: var(--purple); animation: blink .9s step-end infinite; vertical-align: middle; }
    @keyframes blink { 0%,100%{opacity:1} 50%{opacity:0} }

    /* Stats bento */
    .bento-stats { grid-column: 2; grid-row: 1; }
    .bento-stats-grid { display: grid; grid-template-columns: 1fr 1fr; gap: 12px; height: 100%; }
    .stat-item { display: flex; flex-direction: column; gap: 4px; }
    .stat-num { font-size: 2rem; font-weight: 900; line-height: 1; }
    .stat-num.purple { color: var(--purple); }
    .stat-num.green { color: var(--green); }
    .stat-num.blue { color: var(--blue); }
    .stat-num.gold { color: var(--gold); }
    .stat-label { font-size: 11px; color: var(--text2); line-height: 1.3; }

    /* Trust bento */
    .bento-trust { grid-column: 3; grid-row: 1; }
    .bento-label { font-family: var(--mono); font-size: 9px; letter-spacing: .1em; text-transform: uppercase; color: var(--text3); margin-bottom: 14px; }
    .trust-logos { display: flex; flex-direction: column; gap: 10px; }
    .trust-logo { display: flex; align-items: center; gap: 10px; padding: 8px 12px; background: rgba(255,255,255,0.03); border: 1px solid var(--border2); border-radius: 8px; }
    .trust-logo img { height: 22px; object-fit: contain; filter: brightness(0.85) saturate(0.8); }
    .trust-logo span { font-size: 11px; color: var(--text2); }

    /* Guarantee bento */
    .bento-guarantee { grid-column: 2; grid-row: 2; background: linear-gradient(135deg, rgba(168,85,247,0.08), rgba(88,166,255,0.05)); border-color: rgba(168,85,247,0.2); }
    .guarantee-icon { font-size: 28px; margin-bottom: 8px; }
    .guarantee-title { font-size: 14px; font-weight: 700; margin-bottom: 4px; }
    .guarantee-desc { font-size: 12px; color: var(--text2); line-height: 1.5; }

    /* Grads bento */
    .bento-grads { grid-column: 3; grid-row: 2; }
    .grads-faces { display: flex; margin-bottom: 10px; }
    .grad-face { width: 32px; height: 32px; border-radius: 50%; border: 2px solid var(--bg); object-fit: cover; margin-right: -8px; }
    .grads-text { font-size: 13px; font-weight: 600; }
    .grads-sub { font-size: 11px; color: var(--text2); }

    /* â”€â”€ SECTION COMMON â”€â”€ */
    .section { position: relative; z-index: 1; padding: 96px 24px; }
    .section-inner { max-width: 1180px; margin: 0 auto; }
    .section-eyebrow { display: inline-flex; align-items: center; gap: 6px; font-family: var(--mono); font-size: 11px; letter-spacing: .1em; text-transform: uppercase; color: var(--purple); margin-bottom: 16px; opacity: 0; transform: translateY(20px); }
    .section-eyebrow::before { content: ''; width: 20px; height: 1px; background: var(--purple); }
    .section-h2 { font-size: clamp(1.8rem,3.5vw,2.8rem); font-weight: 800; line-height: 1.15; margin-bottom: 16px; opacity: 0; transform: translateY(20px); }
    .section-sub { font-size: 1.05rem; color: var(--text2); max-width: 600px; opacity: 0; transform: translateY(20px); }
    .section-divider { width: 100%; height: 1px; background: linear-gradient(90deg, transparent, var(--border), transparent); margin: 0; }

    /* â”€â”€ PAIN â”€â”€ */
    .pain-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(260px, 1fr)); gap: 16px; margin-top: 48px; }
    .pain-card { padding: 24px; background: rgba(248,81,73,0.04); border: 1px solid rgba(248,81,73,0.15); border-radius: 14px; display: flex; gap: 14px; align-items: flex-start; opacity: 0; transform: translateY(24px); }
    .pain-icon { font-size: 22px; flex-shrink: 0; margin-top: 2px; }
    .pain-title { font-size: 14px; font-weight: 700; margin-bottom: 6px; }
    .pain-desc { font-size: 13px; color: var(--text2); line-height: 1.6; }

    /* â”€â”€ SOLUTION â”€â”€ */
    .solution-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(220px, 1fr)); gap: 16px; margin-top: 48px; }
    .solution-col { padding: 28px 24px; background: rgba(255,255,255,0.025); border: 1px solid var(--border); border-radius: 14px; opacity: 0; transform: translateY(24px); transition: border-color .3s; }
    .solution-col:hover { border-color: rgba(168,85,247,0.3); }
    .sol-num { font-family: var(--mono); font-size: 11px; color: var(--purple); margin-bottom: 12px; letter-spacing: .05em; }
    .sol-title { font-size: 15px; font-weight: 700; margin-bottom: 8px; }
    .sol-desc { font-size: 13px; color: var(--text2); line-height: 1.6; }

    /* â”€â”€ TIMELINE â”€â”€ */
    .timeline { margin-top: 56px; display: flex; flex-direction: column; gap: 0; }
    .tl-item { display: grid; grid-template-columns: 80px 40px 1fr; gap: 0 16px; align-items: flex-start; opacity: 0; transform: translateX(20px); }
    .tl-week { font-family: var(--mono); font-size: 11px; color: var(--text3); text-align: center; padding-top: 16px; }
    .tl-spine { display: flex; flex-direction: column; align-items: center; gap: 0; }
    .tl-dot { width: 12px; height: 12px; border-radius: 50%; background: var(--purple); border: 2px solid var(--bg); flex-shrink: 0; margin-top: 14px; position: relative; z-index: 1; }
    .tl-line { width: 2px; flex: 1; background: var(--border); min-height: 40px; }
    .tl-item:last-child .tl-line { display: none; }
    .tl-content { padding: 12px 0 32px; }
    .tl-title { font-size: 16px; font-weight: 700; margin-bottom: 6px; }
    .tl-desc { font-size: 13px; color: var(--text2); line-height: 1.6; }
    .tl-tags { display: flex; flex-wrap: wrap; gap: 6px; margin-top: 10px; }
    .tl-tag { font-family: var(--mono); font-size: 10px; padding: 3px 8px; background: var(--border2); border: 1px solid var(--border); border-radius: 4px; color: var(--text2); }

    /* â”€â”€ SYLLABUS â”€â”€ */
    .syllabus-explorer { margin-top: 48px; background: rgba(255,255,255,0.02); border: 1px solid var(--border); border-radius: 16px; overflow: hidden; }
    .syl-bar { display: flex; align-items: center; gap: 8px; padding: 10px 16px; background: rgba(0,0,0,0.25); border-bottom: 1px solid var(--border); }
    .syl-bar-title { font-family: var(--mono); font-size: 11px; color: var(--text2); }
    .syl-pane { display: grid; grid-template-columns: 220px 1fr; min-height: 400px; }
    @media(max-width:640px){ .syl-pane { grid-template-columns: 1fr; } .syl-detail { display: none; } }
    .syl-tree { border-left: 1px solid var(--border); padding: 12px 0; overflow-y: auto; max-height: 440px; }
    .syl-item { display: flex; align-items: center; gap: 8px; padding: 7px 16px; font-family: var(--mono); font-size: 12px; color: var(--text2); cursor: pointer; transition: all .2s; border-right: 2px solid transparent; }
    .syl-item:hover, .syl-item.active { color: var(--text); background: rgba(168,85,247,0.08); border-right-color: var(--purple); }
    .syl-item-icon { font-size: 14px; flex-shrink: 0; }
    .syl-item-num { color: var(--text3); font-size: 10px; flex-shrink: 0; }
    .syl-detail { padding: 24px 28px; }
    .syl-detail-num { font-family: var(--mono); font-size: 10px; color: var(--purple); margin-bottom: 8px; letter-spacing: .05em; }
    .syl-detail-title { font-size: 18px; font-weight: 700; margin-bottom: 12px; }
    .syl-detail-desc { font-size: 14px; color: var(--text2); line-height: 1.7; margin-bottom: 16px; }
    .syl-topics { display: flex; flex-direction: column; gap: 6px; }
    .syl-topic { display: flex; align-items: center; gap: 8px; font-size: 13px; color: var(--text2); }
    .syl-topic::before { content: 'â–¸'; color: var(--green); font-size: 11px; }

    /* â”€â”€ HOW IT WORKS â”€â”€ */
    .how-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(240px, 1fr)); gap: 2px; margin-top: 48px; background: var(--border); border-radius: 16px; overflow: hidden; }
    .how-step { padding: 32px 24px; background: var(--bg); transition: background .3s; }
    .how-step:hover { background: rgba(255,255,255,0.02); }
    .how-step-num { font-family: var(--mono); font-size: 32px; font-weight: 700; color: rgba(168,85,247,0.2); margin-bottom: 16px; line-height: 1; }
    .how-step-icon { font-size: 28px; margin-bottom: 12px; }
    .how-step-title { font-size: 16px; font-weight: 700; margin-bottom: 8px; }
    .how-step-desc { font-size: 13px; color: var(--text2); line-height: 1.6; }

    /* â”€â”€ OPERATIONAL BENEFITS â”€â”€ */
    .ops-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(280px, 1fr)); gap: 16px; margin-top: 48px; }
    .ops-card { padding: 24px; background: rgba(255,255,255,0.025); border: 1px solid var(--border); border-radius: 14px; }
    .ops-icon { font-size: 24px; margin-bottom: 12px; }
    .ops-title { font-size: 15px; font-weight: 700; margin-bottom: 8px; }
    .ops-desc { font-size: 13px; color: var(--text2); line-height: 1.6; }
    .ops-badge { display: inline-flex; align-items: center; gap: 6px; background: rgba(63,185,80,0.1); border: 1px solid rgba(63,185,80,0.2); border-radius: 100px; padding: 3px 10px; font-size: 11px; color: var(--green); margin-top: 10px; }

    /* â”€â”€ TRACKS â”€â”€ */
    .tracks-grid { display: grid; grid-template-columns: 1fr 1fr; gap: 16px; margin-top: 48px; }
    @media(max-width:640px){ .tracks-grid { grid-template-columns: 1fr; } }
    .track-card { padding: 28px; background: rgba(255,255,255,0.025); border: 1px solid var(--border); border-radius: 14px; }
    .track-card.featured { border-color: rgba(168,85,247,0.3); background: rgba(168,85,247,0.05); }
    .track-icon { font-size: 28px; margin-bottom: 14px; }
    .track-title { font-size: 18px; font-weight: 800; margin-bottom: 8px; }
    .track-sub { font-size: 13px; color: var(--text2); margin-bottom: 16px; }
    .track-features { display: flex; flex-direction: column; gap: 8px; }
    .track-feat { display: flex; align-items: center; gap: 8px; font-size: 13px; }
    .track-feat-icon { color: var(--green); font-size: 14px; }

    /* â”€â”€ LEARNING PORTAL â”€â”€ */
    .portal-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(220px, 1fr)); gap: 12px; margin-top: 48px; }
    .portal-item { padding: 20px; background: rgba(255,255,255,0.02); border: 1px solid var(--border); border-radius: 12px; opacity: 0; transform: translateY(20px); }
    .portal-item-icon { font-size: 20px; margin-bottom: 10px; }
    .portal-item-title { font-size: 14px; font-weight: 700; margin-bottom: 6px; }
    .portal-item-desc { font-size: 12px; color: var(--text2); line-height: 1.5; }

    /* â”€â”€ BENEFITS â”€â”€ */
    .benefits-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(200px, 1fr)); gap: 16px; margin-top: 48px; }
    .benefit-card { padding: 24px; background: rgba(255,255,255,0.025); border: 1px solid var(--border); border-radius: 14px; text-align: center; transition: all .3s; }
    .benefit-card:hover { transform: translateY(-4px); border-color: rgba(168,85,247,0.3); }
    .benefit-icon { font-size: 32px; margin-bottom: 12px; }
    .benefit-title { font-size: 15px; font-weight: 700; margin-bottom: 6px; }
    .benefit-desc { font-size: 12px; color: var(--text2); line-height: 1.5; }

    /* â”€â”€ FOR WHOM â”€â”€ */
    .whom-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(220px, 1fr)); gap: 16px; margin-top: 48px; }
    .whom-card { padding: 24px; background: rgba(255,255,255,0.025); border: 1px solid var(--border); border-radius: 14px; display: flex; gap: 14px; align-items: flex-start; }
    .whom-icon { font-size: 28px; flex-shrink: 0; }
    .whom-title { font-size: 15px; font-weight: 700; margin-bottom: 6px; }
    .whom-desc { font-size: 13px; color: var(--text2); line-height: 1.5; }

    /* â”€â”€ INSTRUCTORS â”€â”€ */
    .instructors-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(340px, 1fr)); gap: 20px; margin-top: 48px; }
    .instructor-card { padding: 0; background: rgba(255,255,255,0.025); border: 1px solid var(--border); border-radius: 16px; overflow: hidden; display: flex; }
    .instructor-photo { width: 140px; flex-shrink: 0; object-fit: cover; }
    .instructor-body { padding: 24px; flex: 1; }
    .instructor-role { font-family: var(--mono); font-size: 10px; letter-spacing: .1em; text-transform: uppercase; color: var(--purple); margin-bottom: 8px; }
    .instructor-name { font-size: 20px; font-weight: 800; margin-bottom: 10px; }
