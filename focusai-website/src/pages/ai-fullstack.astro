  var hasHover = window.matchMedia('(hover: hover)').matches;

  if (hasHover && cursorGlow) {
    document.addEventListener('mousemove', function(e) {
      mouseX = e.clientX;
      mouseY = e.clientY;
    });
    (function lerpGlow() {
      glowX += (mouseX - glowX) * 0.12;
      glowY += (mouseY - glowY) * 0.12;
      cursorGlow.style.transform = 'translate(' + (glowX - 200) + 'px,' + (glowY - 200) + 'px)';
      requestAnimationFrame(lerpGlow);
    })();
  }

  // ---- 3D CARD TILT (desktop only) ----
  if (hasHover) {
    document.querySelectorAll('.tilt-card').forEach(function(card) {
      card.addEventListener('mousemove', function(e) {
        var rect = card.getBoundingClientRect();
        var x = e.clientX - rect.left;
        var y = e.clientY - rect.top;
        var centerX = rect.width / 2;
        var centerY = rect.height / 2;
        var rotateX = ((y - centerY) / centerY) * -8;
        var rotateY = ((x - centerX) / centerX) * 8;
        card.style.transform = 'perspective(800px) rotateX(' + rotateX + 'deg) rotateY(' + rotateY + 'deg) scale(1.02)';
        var glow = card.querySelector('.card-glow');
        if (glow) {
          glow.style.opacity = '1';
          glow.style.background = 'radial-gradient(circle at ' + x + 'px ' + y + 'px, rgba(168,85,247,0.08), transparent 60%)';
        }
      });
      card.addEventListener('mouseleave', function() {
        card.style.transform = '';
        var glow = card.querySelector('.card-glow');
        if (glow) glow.style.opacity = '0';
      });
    });
  }

  // ---- MAGNETIC BUTTONS (desktop only) ----
  if (hasHover) {
    document.querySelectorAll('.magnetic-btn').forEach(function(btn) {
      btn.addEventListener('mousemove', function(e) {
        var rect = btn.getBoundingClientRect();
        var x = e.clientX - rect.left - rect.width / 2;
        var y = e.clientY - rect.top - rect.height / 2;
        btn.style.transform = 'translate(' + (x * 0.3) + 'px,' + (y * 0.3) + 'px)';
      });
      btn.addEventListener('mouseleave', function() {
        btn.style.transform = '';
        btn.style.transition = 'transform 0.4s cubic-bezier(0.22,1,0.36,1)';
        setTimeout(function() { btn.style.transition = ''; }, 400);
      });
    });
  }

  // ---- FAQ ACCORDION ----
  document.querySelectorAll('.faq-question').forEach(function(btn) {
    btn.addEventListener('click', function() {
      var item = btn.closest('.faq-item');
      var answer = item.querySelector('.faq-answer');
      var isOpen = item.classList.contains('open');

      document.querySelectorAll('.faq-item.open').forEach(function(openItem) {
        openItem.classList.remove('open');
        openItem.querySelector('.faq-answer').style.maxHeight = '0';
      });

      if (!isOpen) {
        item.classList.add('open');
        answer.style.maxHeight = answer.scrollHeight + 'px';
      }
    });
  });

  // ---- FORM ----
  var form = document.getElementById('registerForm');
  var formSuccess = document.getElementById('formSuccess');
  if (form) {
    form.addEventListener('submit', function(e) {
      e.preventDefault();
      var valid = true;

      document.querySelectorAll('.form-error').forEach(function(el) { el.style.display = 'none'; });

      var name = document.getElementById('name');
      var phone = document.getElementById('phone');
      var email = document.getElementById('email');
      var track = document.getElementById('track');
      var privacy = document.getElementById('privacy');
      var marketing = document.getElementById('marketing');
      var background = document.getElementById('background');

      if (!name.value.trim()) { document.getElementById('nameError').style.display = 'block'; valid = false; }
      if (!phone.value.trim() || phone.value.trim().length < 9) { document.getElementById('phoneError').style.display = 'block'; valid = false; }
      if (!email.value.trim() || !/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email.value.trim())) { document.getElementById('emailError').style.display = 'block'; valid = false; }
      if (!track.value) { document.getElementById('trackError').style.display = 'block'; valid = false; }
      if (!privacy.checked) { document.getElementById('privacyError').style.display = 'block'; valid = false; }

      if (!valid) return;

      var submitBtn = form.querySelector('.btn-submit');
      submitBtn.classList.add('loading');
      submitBtn.textContent = 'שולח...';

      var data = {
        name: name.value.trim(),
        phone: phone.value.trim(),
        email: email.value.trim(),
        track: track.value,
        background: background.value.trim(),
        source: 'ai-fullstack',
        marketing_consent: marketing.checked
      };

      fetch('https://n8n.focusai.co.il/webhook/ai-fullstack-register', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify(data)
      }).then(function() {
        form.style.display = 'none';
        formSuccess.style.display = 'block';
        if (typeof fbq === 'function') fbq('track', 'Lead');
      }).catch(function() {
        form.style.display = 'none';
        formSuccess.style.display = 'block';
      }).finally(function() {
        submitBtn.classList.remove('loading');
        submitBtn.textContent = 'שלח הרשמה';
      });
    });
  }

  // ---- GSAP ANIMATIONS ----
  waitForGSAP(function() {
    gsap.registerPlugin(ScrollTrigger);
    var prefersReduced = window.matchMedia('(prefers-reduced-motion: reduce)').matches;
    if (prefersReduced) return;

    // Parallax orbs
    if (window.innerWidth > 768) {
      document.querySelectorAll('.parallax-orb').forEach(function(orb) {
        var speed = parseFloat(orb.dataset.speed) || 0.3;
        gsap.to(orb, {
          y: function() { return -window.innerHeight * speed; },
          ease: 'none',
          scrollTrigger: {
            trigger: document.body,
            start: 'top top',
            end: 'bottom bottom',
            scrub: 1
          }
        });
      });
    }

    // Code typing in hero
    gsap.to('.code-line', {
      opacity: 1,
      x: 0,
      stagger: 0.4,
      duration: 0.6,
      ease: 'power2.out',
      delay: 0.5
    });

    // Counter animation
    document.querySelectorAll('.stat-number').forEach(function(el) {
      var target = parseInt(el.dataset.target);
      var prefix = el.dataset.prefix || '';
      gsap.to({ val: 0 }, {
        val: target,
        duration: 2,
        ease: 'power2.out',
        snap: { val: 1 },
        scrollTrigger: {
          trigger: el,
          start: 'top 80%',
          once: true
        },
        onUpdate: function() {
          el.textContent = prefix + Math.round(this.targets()[0].val);
        }
      });
    });

    // Statement word-by-word reveal
    var statementEl = document.getElementById('statementText');
    if (statementEl) {
      var text = statementEl.textContent.trim();
      var words = text.split(/\s+/);
      statementEl.innerHTML = words.map(function(w) { return '<span class="word">' + w + '</span>'; }).join(' ');

      gsap.to('#statementText .word', {
        opacity: 1,
        stagger: 0.08,
        scrollTrigger: {
          trigger: '#statement',
          start: 'top 60%',
          end: 'bottom 40%',
          scrub: 1
        }
      });
    }

    // Reveal groups
    document.querySelectorAll('.reveal-group').forEach(function(group) {
      var items = group.querySelectorAll('.reveal-item');
      gsap.to(items, {
        opacity: 1,
        y: 0,
        stagger: 0.15,
        duration: 0.8,
        ease: 'power3.out',
        scrollTrigger: {
          trigger: group,
          start: 'top 80%',
          once: true
        }
      });
    });

    // Pinned journey section
    var journeyPin = document.getElementById('journeyPin');
    if (journeyPin && window.innerWidth > 768) {
      var stages = document.querySelectorAll('.journey-stage');
      var screens = document.querySelectorAll('.journey-screen');
      var dots = document.querySelectorAll('.journey-dot');
      var totalStages = stages.length;

      ScrollTrigger.create({
        trigger: journeyPin,
        start: 'top top',
        end: 'bottom bottom',
        pin: '.journey-viewport',
        scrub: 1,
        onUpdate: function(self) {
          var progress = self.progress;
          var activeIndex = Math.min(Math.floor(progress * totalStages), totalStages - 1);

          stages.forEach(function(s, i) {
            s.classList.toggle('active', i === activeIndex);
          });
          screens.forEach(function(s, i) {
            s.classList.toggle('active', i === activeIndex);
          });
          dots.forEach(function(d, i) {
            d.classList.toggle('active', i === activeIndex);
          });
        }
      });
    } else {
      // Mobile: show all stages stacked
      document.querySelectorAll('.journey-stage').forEach(function(s) {
        s.style.position = 'relative';
        s.style.opacity = '1';
        s.style.transform = 'none';
        s.style.marginBottom = '40px';
      });
      var jv = document.querySelector('.journey-visual');
      if (jv) jv.style.display = 'none';
      var jp = document.querySelector('.journey-pin');
      if (jp) jp.style.minHeight = 'auto';
      var jvp = document.querySelector('.journey-viewport');
      if (jvp) { jvp.style.position = 'relative'; jvp.style.height = 'auto'; }
      var jd = document.getElementById('journeyDots');
      if (jd) jd.style.display = 'none';
    }

    // Floating code parallax
    if (window.innerWidth > 1024) {
      document.querySelectorAll('.floating-code').forEach(function(el, i) {
        gsap.to(el, {
          y: -200 - i * 50,
          ease: 'none',
          scrollTrigger: {
            trigger: document.body,
            start: 'top top',
            end: 'bottom bottom',
            scrub: 2
          }
        });
      });
    }

  }); // end waitForGSAP

}); // end DOMContentLoaded
</script>
</body>
</html>
