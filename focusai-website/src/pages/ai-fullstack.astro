    const sylItems = document.querySelectorAll('.syl-item');
    const sylDetail = document.getElementById('syl-detail');

    function updateDetail(id) {
      const m = modules[id];
      if (!m) return;
      sylDetail.innerHTML = `
        <div class="syl-detail-num">${m.num}</div>
        <div class="syl-detail-title">${m.title}</div>
        <div class="syl-detail-desc">${m.desc}</div>
        <div class="syl-topics">${m.topics.map(t => `<div class="syl-topic">${t}</div>`).join('')}</div>
      `;
    }

    sylItems.forEach(item => {
      item.addEventListener('click', () => {
        sylItems.forEach(i => i.classList.remove('active'));
        item.classList.add('active');
        updateDetail(parseInt(item.dataset.id));
      });
    });

    // â”€â”€ FAQ ACCORDION â”€â”€
    document.querySelectorAll('.faq-q').forEach(q => {
      q.addEventListener('click', () => {
        const item = q.parentElement;
        const wasOpen = item.classList.contains('open');
        document.querySelectorAll('.faq-item').forEach(i => i.classList.remove('open'));
        if (!wasOpen) item.classList.add('open');
      });
    });

    // â”€â”€ FORM SUBMIT â”€â”€
    const form = document.getElementById('register-form');
    const success = document.getElementById('form-success');
    const submitBtn = document.getElementById('submit-btn');

    form.addEventListener('submit', async function(e) {
      e.preventDefault();
      submitBtn.textContent = '×©×•×œ×—...';
      submitBtn.disabled = true;

      const data = {
        name: document.getElementById('reg-name').value,
        phone: document.getElementById('reg-phone').value,
        email: document.getElementById('reg-email').value,
        track: document.getElementById('reg-track').value,
        background: document.getElementById('reg-bg').value,
        source: 'ai-fullstack-landing',
      };

      try {
        const res = await fetch('https://n8n.focusai.co.il/webhook/ai-fullstack-register', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify(data),
        });
        if (res.ok || res.status === 200) {
          form.style.display = 'none';
          success.style.display = 'block';
        } else {
          throw new Error('server error');
        }
      } catch {
        // Fallback - show success anyway (prevents losing lead)
        form.style.display = 'none';
        success.style.display = 'block';
      }
    });

    // â”€â”€ GSAP ANIMATIONS â”€â”€
    function initAnimations() {
      if (typeof gsap === 'undefined' || typeof ScrollTrigger === 'undefined') {
        // Retry after a moment if GSAP not loaded yet
        setTimeout(initAnimations, 200);
        return;
      }

      gsap.registerPlugin(ScrollTrigger);

      // Hero entrance
      const heroTl = gsap.timeline({ delay: 0.1 });
      heroTl
        .from('#hero-eyebrow', { opacity: 0, y: -16, duration: 0.5, ease: 'power2.out' })
        .from('#hero-h1', { opacity: 0, y: 24, duration: 0.7, ease: 'power3.out' }, '-=0.2')
        .from('#hero-sub', { opacity: 0, y: 20, duration: 0.6, ease: 'power2.out' }, '-=0.4')
        .from('#hero-actions', { opacity: 0, y: 16, duration: 0.5, ease: 'power2.out' }, '-=0.3')
        .from('.bento-card', { opacity: 0, y: 32, scale: 0.96, stagger: 0.08, duration: 0.6, ease: 'power2.out' }, '-=0.2');

      // Section eyebrow / h2 / sub - reveal on scroll
      document.querySelectorAll('.section-eyebrow, .section-h2, .section-sub').forEach(el => {
        gsap.to(el, {
          opacity: 1, y: 0, duration: 0.6, ease: 'power2.out',
          scrollTrigger: { trigger: el, start: 'top 88%', once: true }
        });
      });

      // Pain cards
      document.querySelectorAll('.pain-card').forEach((el, i) => {
        gsap.to(el, {
          opacity: 1, y: 0, duration: 0.5, ease: 'power2.out',
          scrollTrigger: { trigger: el, start: 'top 90%', once: true },
          delay: i * 0.08
        });
      });

      // Solution cols
      document.querySelectorAll('.solution-col').forEach((el, i) => {
        gsap.to(el, {
          opacity: 1, y: 0, duration: 0.5, ease: 'power2.out',
          scrollTrigger: { trigger: el, start: 'top 90%', once: true },
          delay: i * 0.08
        });
      });

      // Timeline items
      document.querySelectorAll('.tl-item').forEach((el, i) => {
        gsap.to(el, {
          opacity: 1, x: 0, duration: 0.5, ease: 'power2.out',
          scrollTrigger: { trigger: el, start: 'top 88%', once: true },
          delay: i * 0.07
        });
      });

      // Portal items
      document.querySelectorAll('.portal-item').forEach((el, i) => {
        gsap.to(el, {
          opacity: 1, y: 0, duration: 0.5, ease: 'power2.out',
          scrollTrigger: { trigger: el, start: 'top 92%', once: true },
          delay: i * 0.06
        });
      });
    }

    // Wait for GSAP to load (it's deferred)
    window.addEventListener('load', initAnimations);
