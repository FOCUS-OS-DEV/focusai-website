  <style>
    * { margin: 0; padding: 0; box-sizing: border-box; }

    :root {
      --bg: #080810;
      --surface: #12121a;
      --primary: #a855f7;
      --primary-glow: rgba(168, 85, 247, 0.3);
      --green: #22c55e;
      --green-glow: rgba(34, 197, 94, 0.3);
      --text: #e8e0ff;
      --text-secondary: #9590b0;
      --border: rgba(255,255,255,0.08);
    }

    body {
      font-family: 'Heebo', sans-serif;
      background-color: var(--bg);
      color: var(--text);
      min-height: 100vh;
      direction: rtl;
      overflow-x: hidden;
    }

    /* Background glow */
    .bg-glow {
      position: fixed;
      top: -30%;
      left: 50%;
      transform: translateX(-50%);
      width: 600px;
      height: 600px;
      background: radial-gradient(circle, rgba(168,85,247,0.08) 0%, transparent 70%);
      pointer-events: none;
      z-index: 0;
    }

    .page-wrap {
      position: relative;
      z-index: 1;
      max-width: 640px;
      margin: 0 auto;
      padding: 40px 20px 60px;
      min-height: 100vh;
      display: flex;
      flex-direction: column;
      align-items: center;
    }

    /* Logo */
    .logo {
      margin-bottom: 40px;
    }
    .logo img {
      height: 36px;
      width: auto;
    }

    /* Success Icon */
    .success-icon {
      width: 80px;
      height: 80px;
      border-radius: 50%;
      background: linear-gradient(135deg, var(--green), #16a34a);
      display: flex;
      align-items: center;
      justify-content: center;
      margin: 0 auto 24px;
      box-shadow: 0 0 40px var(--green-glow);
      animation: scaleIn 0.5s cubic-bezier(0.34, 1.56, 0.64, 1) forwards;
    }
    .success-icon i {
      font-size: 36px;
      color: #fff;
    }

    @keyframes scaleIn {
      from { transform: scale(0); opacity: 0; }
      to { transform: scale(1); opacity: 1; }
    }

    /* Title */
    .success-title {
      font-size: 2rem;
      font-weight: 800;
      text-align: center;
      margin-bottom: 8px;
      animation: fadeUp 0.6s 0.2s both;
    }
    .success-subtitle {
      font-size: 1rem;
      color: var(--text-secondary);
      text-align: center;
      margin-bottom: 32px;
      animation: fadeUp 0.6s 0.3s both;
    }

    @keyframes fadeUp {
      from { opacity: 0; transform: translateY(16px); }
      to { opacity: 1; transform: translateY(0); }
    }

    /* Event Details Card */
    .event-card {
      width: 100%;
      background: rgba(255,255,255,0.03);
      backdrop-filter: blur(20px);
      -webkit-backdrop-filter: blur(20px);
      border: 1px solid var(--border);
      border-radius: 16px;
      padding: 24px;
      margin-bottom: 24px;
      animation: fadeUp 0.6s 0.4s both;
    }
    .event-card-title {
      font-size: 0.75rem;
      font-weight: 700;
      color: var(--primary);
      letter-spacing: 2px;
      text-transform: uppercase;
      margin-bottom: 16px;
      text-align: center;
    }
    .detail-row {
      display: flex;
      align-items: center;
      gap: 12px;
      padding: 10px 0;
      border-bottom: 1px solid rgba(255,255,255,0.04);
    }
    .detail-row:last-child {
      border-bottom: none;
    }
    .detail-row i {
      width: 20px;
      text-align: center;
      color: var(--primary);
      font-size: 0.9rem;
    }
    .detail-row span {
      font-size: 0.95rem;
      color: var(--text);
    }

    /* WhatsApp CTA - Main highlight */
    .whatsapp-cta {
      width: 100%;
      background: linear-gradient(135deg, rgba(34,197,94,0.1), rgba(22,163,74,0.05));
      border: 1.5px solid rgba(34,197,94,0.3);
      border-radius: 16px;
      padding: 28px 24px;
      margin-bottom: 24px;
      text-align: center;
      animation: fadeUp 0.6s 0.5s both;
    }
    .whatsapp-cta .cta-icon {
      width: 56px;
      height: 56px;
      border-radius: 50%;
      background: linear-gradient(135deg, #22c55e, #16a34a);
      display: flex;
      align-items: center;
      justify-content: center;
      margin: 0 auto 16px;
      box-shadow: 0 0 30px rgba(34,197,94,0.3);
    }
    .whatsapp-cta .cta-icon i {
      font-size: 28px;
      color: #fff;
    }
    .whatsapp-cta h3 {
      font-size: 1.3rem;
      font-weight: 800;
      margin-bottom: 8px;
      color: #fff;
    }
    .whatsapp-cta p {
      font-size: 0.9rem;
      color: var(--text-secondary);
      line-height: 1.7;
      margin-bottom: 20px;
    }
    .whatsapp-cta .highlight {
      color: #4ade80;
      font-weight: 700;
    }
    .whatsapp-btn {
      display: inline-flex;
      align-items: center;
      gap: 10px;
      background: linear-gradient(135deg, #22c55e, #16a34a);
      color: #fff;
      font-size: 1.05rem;
      font-weight: 700;
      padding: 14px 36px;
      border-radius: 12px;
      text-decoration: none;
      transition: all 0.3s;
      box-shadow: 0 4px 20px rgba(34,197,94,0.3);
    }
    .whatsapp-btn:hover {
      transform: translateY(-2px);
      box-shadow: 0 8px 30px rgba(34,197,94,0.4);
    }
    .whatsapp-btn i {
      font-size: 1.2rem;
    }

    /* Links Grid */
    .links-section {
      width: 100%;
      margin-bottom: 24px;
      animation: fadeUp 0.6s 0.6s both;
    }
    .links-title {
      font-size: 0.75rem;
      font-weight: 700;
      color: var(--text-secondary);
      letter-spacing: 2px;
      text-transform: uppercase;
      margin-bottom: 14px;
      text-align: center;
    }
    .links-grid {
      display: grid;
      grid-template-columns: 1fr 1fr;
      gap: 10px;
    }
    .link-card {
      display: flex;
      align-items: center;
      gap: 10px;
      padding: 14px 16px;
      background: rgba(255,255,255,0.03);
      border: 1px solid var(--border);
      border-radius: 12px;
      text-decoration: none;
      color: var(--text);
      transition: all 0.3s;
    }
    .link-card:hover {
      background: rgba(255,255,255,0.06);
      border-color: rgba(168,85,247,0.3);
      transform: translateY(-1px);
    }
    .link-card i {
      font-size: 1.1rem;
      color: var(--primary);
      width: 20px;
      text-align: center;
    }
    .link-card span {
      font-size: 0.85rem;
      font-weight: 600;
    }

    /* Social links */
    .social-section {
      display: flex;
      align-items: center;
      gap: 16px;
      margin-bottom: 32px;
      animation: fadeUp 0.6s 0.7s both;
    }
    .social-link {
      width: 42px;
      height: 42px;
      border-radius: 50%;
      background: rgba(255,255,255,0.04);
      border: 1px solid var(--border);
      display: flex;
      align-items: center;
      justify-content: center;
      color: var(--text-secondary);
      text-decoration: none;
      transition: all 0.3s;
      font-size: 1rem;
    }
    .social-link:hover {
      background: rgba(168,85,247,0.1);
      border-color: rgba(168,85,247,0.3);
      color: var(--primary);
    }

    /* Footer */
    .page-footer {
      text-align: center;
      animation: fadeUp 0.6s 0.8s both;
    }
    .page-footer p {
      font-size: 0.8rem;
      color: rgba(255,255,255,0.25);
    }
    .page-footer a {
      color: var(--primary);
      text-decoration: none;
    }

    /* Back link */
    .back-link {
      display: inline-flex;
      align-items: center;
      gap: 6px;
      color: var(--text-secondary);
      font-size: 0.85rem;
      text-decoration: none;
      margin-bottom: 30px;
      transition: color 0.3s;
      animation: fadeUp 0.5s 0.1s both;
    }
    .back-link:hover {
      color: var(--primary);
    }
    .back-link i {
      font-size: 0.75rem;
    }

    /* Confetti */
    .confetti-container {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      pointer-events: none;
      z-index: 10;
      overflow: hidden;
    }
    .confetti {
      position: absolute;
      width: 8px;
      height: 8px;
      border-radius: 2px;
      top: -10px;
      animation: confettiFall linear forwards;
    }
    @keyframes confettiFall {
      0% { transform: translateY(0) rotate(0deg); opacity: 1; }
      100% { transform: translateY(100vh) rotate(720deg); opacity: 0; }
    }

    @media (max-width: 480px) {
      .page-wrap {
        padding: 30px 16px 50px;
      }
      .success-title {
        font-size: 1.6rem;
      }
      .links-grid {
        grid-template-columns: 1fr;
      }
      .whatsapp-cta h3 {
        font-size: 1.15rem;
      }
    }
  </style>
</head>
